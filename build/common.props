<Project>

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <PackageLicenseUrl>https://github.com/prayzzz/prz.Common/blob/master/LICENSE</PackageLicenseUrl>
        <PackageProjectUrl>https://github.com/prayzzz/prz.Common</PackageProjectUrl>
        <RepositoryType>git</RepositoryType>
        <RepositoryUrl>https://github.com/prayzzz/prz.Common</RepositoryUrl>
        <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
        <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
        <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    </PropertyGroup>

    <Target Name="PrintVersion" BeforeTargets="BeforeBuild">
        <Message Text="Building $(AssemblyName) version $(Version)" Importance="High"/>
    </Target>

</Project>